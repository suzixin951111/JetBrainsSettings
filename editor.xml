<application>
  <component name="CodeInsightSettings">
    <option name="PARAMETER_INFO_DELAY" value="300" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="300" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="LINE_SPACING" value="1.0" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern useCustomColors="true" pattern="注意\b.">
      <option name="FOREGROUND" value="c00002" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bNotice\b.">
      <option name="FOREGROUND" value="c00013" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="注\b">
      <option name="FOREGROUND" value="9c000" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
</application>